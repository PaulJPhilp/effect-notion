{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "effect-notion",
  "version": "1.0.0",
  "module": "src/main.ts",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "bun src/main.ts",
    "dev": "bun --watch src/main.ts",
    "test": "bun test",
    "test:coverage": "vitest run --coverage",
    "lint": "bunx @biomejs/biome check .",
    "format": "bunx @biomejs/biome format --write .",
    "typecheck": "bun tsc --noEmit",
    "build": "bun tsc",
    "codegen:notion": "bun scripts/generate-notion-schema.ts"
  },
  "devDependencies": {
    "@effect/test": "^0.1.0",
    "@types/bun": "latest",
    "@types/marked": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@effect/platform": "^0.90.10",
    "@effect/platform-node": "^0.96.1",
    "@effect/schema": "^0.75.5",
    "dotenv": "^17.2.3",
    "effect": "^3.18.4",
    "marked": "^16.3.0",
    "vercel": "^48.2.1"
  }
}
